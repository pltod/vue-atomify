{
  "name": "atomify-vue-example",
  "version": "0.0.1",
  "description": "An example app built with atomify and vue.js",
  "scripts": {
    "serve": "atomify --server [ --path index.html --open ]"
  },
  "atomify": {
    "js": {
      "entry": "src/app.js",
      "alias": "/bundle.js"
    },
    "css": {
      "entry": "src/app.css",
      "alias": "/bundle.css"
    }
  },
  "keywords": [
    "atomify",
    "vuejs"
  ],
  "author": "pltod",
  "license": "MIT",
  "dependencies": {
    "vue": "^0.10.3"
  },
  "devDependencies": {
    "gulp": "^3.6.2",
    "atomify": "^1.4.4",
    "ios-overlay": "^0.0.1",
    "st": "^0.4.1"
  }
  
}
